			READ_2DA_ENTRY i 0 2 kitidx
			LPF TO_DEC_NUMBER STR_VAR hexNumber = EVAL ~%kitidx%~ RET value END
			READ_2DA_ENTRY i 1 2 kit
			TO_UPPER kit
			PATCH_IF NOT VARIABLE_IS_SET $CHECK_SCRIPT_KITIDS(~kit_%value%~) BEGIN
